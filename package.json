{
  "name": "arco-design-pro-vue",
  "description": "Arco Design Pro for Vue",
  "version": "1.0.0",
  "private": true,
  "author": "ArcoDesign Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "build": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts",
    "build:notsc": "vite build --config ./config/vite.config.prod.ts",
    "report": "cross-env REPORT=true npm run build",
    "preview": "npm run build && vite preview --host",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint-staged": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.49.3",
    "@arco-themes/vue-powerx-design-default": "^0.0.2",
    "@babel/core": "^7.0.0",
    "@fullcalendar/core": "^6.1.6",
    "@fullcalendar/daygrid": "^6.1.6",
    "@fullcalendar/interaction": "^6.1.6",
    "@fullcalendar/timegrid": "^6.1.6",
    "@fullcalendar/vue3": "^6.1.6",
    "@vueuse/core": "^9.3.0",
    "@yaoyaochi/weyui": "^1.1.9",
    "axios": "^0.24.0",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "query-string": "^8.0.3",
    "sortablejs": "^1.15.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-qr": "^4.0.9",
    "vue-router": "^4.0.14",
    "wujie-vue3": "^1.0.18"
  },
  "devDependencies": {
    "@arco-plugins/vite-vue": "^1.4.5",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/lodash": "^4.14.186",
    "@types/mockjs": "^1.0.7",
    "@types/nprogress": "^0.2.0",
    "@types/sortablejs": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vitejs/plugin-vue": "4.4.0",
    "@vitejs/plugin-vue-jsx": "3.0.2",
    "@vue/babel-plugin-jsx": "1.1.5",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.17.0",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "rollup": "3.29.4",
    "rollup-plugin-visualizer": "5.9.2",
    "stylelint": "^15.10.3",
    "stylelint-config-rational-order-fix": "^0.1.9",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.2.2",
    "unplugin-vue-components": "^0.25.2",
    "vite": "4.4.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.19"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  }
}
